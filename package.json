{
  "name": "sm-waveform",
  "version": "0.1.0",
  "description": "Generate waveform data with ffmpeg",
  "keywords": [
    "streammachine"
  ],
  "license": "MIT",
  "homepage": "https://github.com/StreamMachine/sm-waveform/",
  "author": "Eric Richardson <e@ewr.is>",
  "repository": {
    "type": "git",
    "url": "https://github.com/StreamMachine/sm-waveform.git"
  },
  "main": "./build/index.js",
  "scripts": {
    "start": "node ./build/main.js",
    "dev": "backpack dev $NODE_DEBUG_OPTION",
    "build": "backpack build",
    "test": "NODE_ENV=test jest "
  },
  "dependencies": {
    "fluent-ffmpeg": "^2.0.1",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.6",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.5.5",
    "awesome-typescript-loader": "^3.5.0",
    "babel-jest": "^23.4.2",
    "backpack-core": "^0.7.0",
    "jest-cli": "^23.4.2",
    "ts-jest": "^23.1.2",
    "typescript": "^3.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
